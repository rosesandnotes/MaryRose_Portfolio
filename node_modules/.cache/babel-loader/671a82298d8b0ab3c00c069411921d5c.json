{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport { ref, computed } from 'vue';\nimport getWorks from '../data/getWorks';\nexport default {\n  setup() {\n    const route = useRoute();\n    const routeId = ref(route.params.id);\n    const {\n      works\n    } = getWorks();\n    const filteredWorkOnId = computed(() => {\n      return works.value.filter(item => item.workID == routeId.value);\n    });\n    return {\n      route,\n      works,\n      filteredWorkOnId\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAmHA,SAAQA,QAAR,QAAuB,YAAvB;AACA,SAAQC,GAAR,EAAaC,QAAb,QAA4B,KAA5B;AACA,OAAOC,QAAP,MAAqB,kBAArB;AAEE,eAAe;AACbC,OAAK,GAAG;AACN,UAAMC,KAAI,GAAIL,QAAQ,EAAtB;AACA,UAAMM,OAAM,GAAIL,GAAG,CAACI,KAAK,CAACE,MAAN,CAAaC,EAAd,CAAnB;AAEA,UAAM;AAACC;AAAD,QAAUN,QAAQ,EAAxB;AAEA,UAAMO,gBAAe,GAAIR,QAAQ,CAAE,MAAM;AACvC,aAAOO,KAAK,CAACE,KAAN,CAAYC,MAAZ,CAAoBC,IAAG,IAAKA,IAAI,CAACC,MAAL,IAAeR,OAAO,CAACK,KAAnD,CAAP;AACD,KAFgC,CAAjC;AAIA,WAAM;AACJN,WADI;AAEJI,WAFI;AAGJC;AAHI,KAAN;AAMF;;AAjBa,CAAf","names":["useRoute","ref","computed","getWorks","setup","route","routeId","params","id","works","filteredWorkOnId","value","filter","item","workID"],"sourceRoot":"","sources":["/Users/maryroseavenido/Documents/Multimedia Design/Development/2nd Sem/Portfolio/portfolio-code/portfolio/src/views/WorkDetailsView.vue"],"sourcesContent":["<template>\n  <div class=\"workdetails-section overflow-hidden\" >\n    <div v-for=\"work in filteredWorkOnId\" :key=\"work.workID\">\n         <div class=\"title-section d-flex align-items-center justify-content-center py-5\">\n          <div class=\"container d-flex flex-column align-items-center justify-content-center \" >\n            <div class=\"row gx-5 py-3 d-flex align-items-center justify-content-center\">\n              <h1 class=\"mb-3\">\n              {{ work.workTitle }}\n              </h1>\n              <h2 class=\"mb-4\">\n               {{ work.workCategory }}\n              </h2>\n              <!-- <img :src=\"work.mockupImage\" :alt=\"work.mockUpImageAlt\" width=\"850\" height=\"500\"> -->\n            </div>\n            <div class=\"col-xl-10 col-12\">\n                <img :src=\"work.mockupImage\" :alt=\"work.mockUpImageAlt\" class=\"mockupImage\">\n            </div>\n            \n          </div>\n      </div>\n      <div class=\"details-section gx-5 py-5 d-flex align-items-center justify-content-between\">\n        <div class=\"container\">\n          <div class=\"row\">\n              <div class=\"col-8 d-flex flex-column align-items-start justify-content-start\">\n                <h2 class=\"mb-4\">\n                      Project Brief\n                    </h2>\n                    <p class=\"mb-5\">\n                      {{work.projectBrief}}\n                    </p>\n              </div>\n          \n          </div>\n          <div class=\"row\">\n            <div class=\"col-12 d-flex align-items-center justify-content-start\">\n                <div class=\"colors d-flex flex-column col-xl-4 col-m-4 col-12 align-items-start justify-content-start\">\n                    <h2 class=\"mb-4\">\n                      Colors\n                    </h2>\n                    <div class=\"colors-container d-flex mb-5 \">\n                      <div class=\"color-group d-flex flex-column align-items-center justify-content-center\" v-for=\"color in work.colors\" :key=\"color\" :style=\"color.stylePosition\" >\n                          <div class=\"color-image mb-2\" :style=\"color.styleBackground\" >\n\n                          </div>\n                          <div class=\"color-desc first-color-text\">\n                            <p>\n                            {{color.colorID}}\n                            </p>\n                          </div>\n                      </div>\n                    </div>\n                </div>\n                \n            <!-- </div>\n            <div class=\"typography-section col-xl-8 d-flex flex-column align-items-start justify-content-start mb-5\"> -->\n              <div class=\"typography d-flex flex-column col-xl-4 col-m-4 col-12 align-items-start justify-content-start\">\n                    <h2 class=\"mb-4 \">\n                      Typography\n                    </h2>\n                    <div class=\"typo-container first-typo d-flex alig-items-center justify-content-between flex-wrap\" :style=\"work.primaryFontStyle.fontFamily\">\n                      <p class=\"typo-title me-4 \">\n                        {{work.primaryFontStyle.fontName}}\n                      </p>\n                      <p class=\"typo-letters mb-5\" >\n                        ABCDEFGHIJKLMNOPQRSTUVWXYZ\n                        \n                      <span v-if=\"work.primaryFontStyle.lowercaseAlphabet\"> <br> abcdefghijklmnopqrstuvwxyz </span>\n                        <br>\n                        0123456789 \n                      </p>\n                    </div>\n                    <div v-if=\"work.secondaryFontAvailable\" class=\"typo-container second-typo d-flex alig-items-center justify-content-between flex-wrap\" :style=\"work.secondaryFontStyle.fontFamily\">\n                      <p class=\"typo-title me-4\">\n                      {{work.secondaryFontStyle.fontName}}\n                      </p>\n                      <p class=\"typo-letters\">\n                        ABCDEFGHIJKLMNOPQRSTUVWXYZ\n                      <span v-if=\"work.secondaryFontStyle.lowercaseAlphabet\"> <br> abcdefghijklmnopqrstuvwxyz</span>\n                        <br>\n                        0123456789 \n                      </p>\n                    </div>\n\n              </div>\n                \n                <div class=\"tool d-flex flex-column col-xl-4 col-m-4 col-12 align-items-start justify-content-start\">\n                      <h2>\n                        Tool\n                      </h2>\n                    <div v-if=\"work.toolAdobeXD\" class=\"tool-image-xd mb-4 d-flex flex-column align-items-start justify-content-start\">\n                    </div>\n                    <div v-if=\"work.toolAI\" class=\"tool-image-ai mb-4 d-flex flex-column align-items-start justify-content-start\">\n                    </div>\n                </div>\n            </div>\n            \n          </div>\n            \n        </div>\n      </div>\n      <div class=\"pages-section\">\n              <img :src=\"work.pagesMockupImage\" :alt=\"pagesMockupImageAlt\" class=\"pages-image\">\n      </div>   \n      <div class=\"onepage-section gx-5 py-5 d-flex flex-column align-items-center justify-content-center\">\n        <h2 class=\"mb-4\">\n          {{work.onePageTitle}}\n        </h2>\n        <img :src=\"work.onePageImage\" :alt=\"work.onePageImageAlt\" class=\"onepage-image\">\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\nimport {useRoute} from 'vue-router'\nimport {ref, computed} from 'vue'\nimport getWorks from '../data/getWorks'\n\n  export default {\n    setup() {\n      const route = useRoute()\n      const routeId = ref(route.params.id)\n\n      const {works} = getWorks();\n\n      const filteredWorkOnId = computed( () => {\n        return works.value.filter( item => item.workID == routeId.value)\n      })\n\n      return{\n        route,\n        works,\n        filteredWorkOnId\n      }\n      \n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n.workdetails-section{\n  min-height: 80vh;\n\n}\n\n.title-section{\n  background-color:  #1A2C26;\n  h2, h1{\n      color: #D7E0DD;\n  }\n}\n\nh1{\n  font-weight: bold;\n}\n\n\n.details-section{\n  background-color: #D7E0DD;\n  \n}\n\nh2{\n    font-weight: 700;\n  }\n\n.colors-container{\n  position: relative;\n  p {\n    font-size: 12px;\n  }\n}\n\n// .first-color-image{\n//   // background-color: #B98C87;\n// }\n\n// .second-color-image{\n//   background-color: #674746;\n// }\n\n// .third-color-image{\n//   background-color: #E6746C;\n// }\n\n// .second-color-group\n// {\n//   position:absolute;\n//   // left: 80px;\n// }\n\n// .third-color-group\n// {\n//   position:absolute;\n//   left: 160px;\n// }\n\n\n.color-image{\n  height: 100px;\n  width: 100px;\n  border-radius: 50px;\n}\n\n.color-desc{\n  font-weight: 700;\n  text-align: center;\n}\n\n.tool-image-xd{\n  background-image: url(\"../assets/adobexd.svg\");\n  width: 120px;\n  height: 120px;\n  background-position: center;\n  background-repeat: no-repeat;\n  background-size: cover;\n}\n\n.tool-image-ai{\n  background-image: url(\"../assets/ai-logo.png\");\n  width: 120px;\n  height: 120px;\n  background-position: center;\n  background-repeat: no-repeat;\n  background-size: cover;\n}\n\n.first-typo{\n  h3{\n    // font-family: 'Playfair Display', serif;\n    font-weight: 400;\n    text-decoration: none;\n    text-transform: none;\n    text-align: left;\n    font-size: 24px;\n  }\n}\n\n.pages-image{\n  width: 100vw;\n}\n\n.onepage-image{\n  width: 80vw;\n}\n\n.mockupImage{\n  width: 100%;\n}\n\n\n</style>"]},"metadata":{},"sourceType":"module"}